  "devDependencies": {
    "@taroxin/eslint-config": "*",
    "bumpp": "^8.2.1",
    "eslint": "^8.29.0",
    "eslint-plugin-taroxin": "workspace:*",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  }
}
